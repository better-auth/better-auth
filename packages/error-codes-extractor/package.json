{
	"name": "@better-auth/error-codes-extractor",
	"version": "1.5.0-beta.10",
	"type": "module",
	"description": "Rolldown/Rollup plugin to extract and document error codes from Better Auth",
	"module": "dist/index.mjs",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/better-auth/better-auth.git",
		"directory": "packages/error-codes-extractor"
	},
	"homepage": "https://www.better-auth.com",
	"main": "./dist/index.mjs",
	"scripts": {
		"build": "tsdown",
		"dev": "tsdown --watch",
		"typecheck": "tsc --project tsconfig.json"
	},
	"publishConfig": {
		"access": "public"
	},
	"license": "MIT",
	"keywords": [
		"better-auth",
		"error-codes",
		"documentation",
		"rolldown",
		"rollup",
		"plugin",
		"typescript"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		}
	},
	"typesVersions": {
		"*": {
			"*": [
				"./dist/index.d.mts"
			]
		}
	},
	"files": [
		"dist"
	],
	"devDependencies": {
		"tsdown": "catalog:",
		"typescript": "catalog:"
	},
	"peerDependencies": {
		"rolldown": ">=1.0.0-rc.1",
		"typescript": "^5.0.0"
	},
	"peerDependenciesMeta": {
		"rolldown": {
			"optional": true
		}
	}
}
