    "expo-crypto": "^15.0.7",
    "expo-linking": "~8.0.8",
    "expo-secure-store": "~15.0.7",
    "expo-web-browser": "~15.0.7",
    "unbuild": "^3.6.1"
  },
  "peerDependencies": {
    "better-auth": "workspace:*",
    "expo-constants": ">=17.0.0",
