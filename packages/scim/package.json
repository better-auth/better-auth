{
  "name": "@better-auth/scim",
  "author": "Jonathan Samines",
  "version": "1.4.0-beta.13",
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "keywords": [
    "auth",
    "scim"
  ],
  "publishConfig": {
    "access": "public"
  },
  "module": "dist/index.js",
  "description": "SCIM plugin for Better Auth",
  "scripts": {
    "test": "vitest",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --project tsconfig.json"
  },
  "exports": {
    ".": {
      "dev-source": "./src/index.ts",
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "@better-auth/utils": "0.3.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@better-auth/sso": "workspace:*",
    "tsdown": "catalog:"
  },
  "peerDependencies": {
    "better-auth": "workspace:*"
  }
}
