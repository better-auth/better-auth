      "db/adapter": [
        "dist/db/adapter/index.d.ts"
      ],
      "oauth2": [
        "dist/oauth2/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "unbuild --clean",
    "stub": "unbuild --stub",
    "typecheck": "tsc --project tsconfig.json"
  },
  "devDependencies": {
    "@better-auth/utils": "0.3.0",
    "@better-fetch/fetch": "catalog:",
    "@types/better-sqlite3": "^7.6.13",
    "better-call": "catalog:",
