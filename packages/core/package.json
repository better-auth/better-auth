      },
      "require": {
        "types": "./dist/env/index.d.cts",
        "default": "./dist/env/index.cjs"
      }
    },
    "./error": {
      "import": {
        "types": "./dist/error/index.d.ts",
        "default": "./dist/error/index.mjs"
      },
      "require": {
        "types": "./dist/error/index.d.cts",
