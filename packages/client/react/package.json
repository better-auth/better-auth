{
  "name": "@better-auth/react",
  "version": "1.4.0-beta.2",
  "description": "React client for Better Auth",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/better-auth/better-auth",
    "directory": "packages/client/react"
  },
  "keywords": [
    "auth",
    "react",
    "authentication",
    "typescript"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "unbuild --clean",
    "dev": "unbuild --watch",
    "typecheck": "tsc --noEmit"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "dependencies": {
    "@better-auth/client-core": "workspace:*",
    "nanostores": "^1.0.1"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.23",
    "better-auth": "workspace:*",
    "react": "^19.1.1",
    "typescript": "catalog:",
    "unbuild": "catalog:"
  },
  "files": [
    "dist"
  ]
}
